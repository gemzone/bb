<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.jshio.breadboard.domain">
	<class name="User" table="gz_user" catalog="bb_core" schema="dbo">
		<id name="userId" column="user_id" />
		<property name="name" column="name" />
		<property name="comment" column="comment" />
		<property name="permission" column="permission" />
		<property name="admin" column="admin" />
		<property name="creationTime" column="creation_time" />
		<property name="oauthNaverId" column="oauth_naver_id" />
	</class>
</hibernate-mapping>
